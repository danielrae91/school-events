# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here

# Upstash Redis Configuration
KV_REST_API_URL=https://your-redis-url.upstash.io
KV_REST_API_TOKEN=your-redis-token-here
KV_REST_API_READ_ONLY_TOKEN=your-redis-read-only-token-here

# Admin Panel Access
ADMIN_TOKEN=your-admin-token-here

# Google Calendar Integration (optional)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=https://yourdomain.com/auth/callback
GOOGLE_REFRESH_TOKEN=your_refresh_token
GOOGLE_CALENDAR_ID=your_calendar_id

# Sync secret for manual calendar sync
SYNC_SECRET=your_sync_secret

# CloudMailin webhook secret (optional)
CLOUDMAILIN_SECRET=your_cloudmailin_secret

# Push Notification VAPID Keys (required for push notifications)
VAPID_PUBLIC_KEY=your_vapid_public_key
VAPID_PRIVATE_KEY=your_vapid_private_key-here
NEXT_PUBLIC_VAPID_PUBLIC_KEY=your_vapid_public_key
